<odoo>

    <record id="stage_identify" model="mgmt.risk.stage">
        <field name="name">Identify</field>
        <field name="code">identify</field>
        <field name="sequence">1</field>
    </record>

    <record id="stage_evaluate" model="mgmt.risk.stage">
        <field name="name">Evaluate</field>
        <field name="code">evaluate</field>
        <field name="sequence">2</field>
    </record>

    <record id="stage_mitigate" model="mgmt.risk.stage">
        <field name="name">Mitigate</field>
        <field name="code">mitigate</field>
        <field name="sequence">3</field>
    </record>

    <record id="demo_severity1" model="mgmt.severity">
        <field name="name">Very Low</field>
        <field name="value">1</field>
    </record>

    <record id="demo_severity2" model="mgmt.severity">
        <field name="name">Low</field>
        <field name="value">2</field>
    </record>

    <record id="demo_severity3" model="mgmt.severity">
        <field name="name">Medium</field>
        <field name="value">3</field>
    </record>

    <record id="demo_severity4" model="mgmt.severity">
        <field name="name">High</field>
        <field name="value">4</field>
    </record>

    <record id="demo_severity5" model="mgmt.severity">
        <field name="name">Very High</field>
        <field name="value">5</field>
    </record>

    <record id="demo_probability1" model="mgmt.probability">
        <field name="name">Very Low</field>
        <field name="value">1</field>
    </record>

    <record id="demo_probability2" model="mgmt.probability">
        <field name="name">Low</field>
        <field name="value">2</field>
    </record>

    <record id="demo_probability3" model="mgmt.probability">
        <field name="name">Medium</field>
        <field name="value">3</field>
    </record>

    <record id="demo_probability4" model="mgmt.probability">
        <field name="name">High</field>
        <field name="value">4</field>
    </record>

    <record id="demo_probability5" model="mgmt.probability">
        <field name="name">Very High</field>
        <field name="value">5</field>
    </record>

</odoo>
