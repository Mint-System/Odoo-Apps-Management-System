<?xml version="1.0" encoding="utf-8" ?>
<odoo>

    <record id="severity_view_tree" model="ir.ui.view">
        <field name="name">mgmt_risk.severity_tree_view</field>
        <field name="model">mgmt.severity</field>
        <field name="arch" type="xml">
            <tree editable="top">
                <field name="name" />
            </tree>
        </field>
    </record>

    <record id="action_mgmt_severity_view" model="ir.actions.act_window">
      <field name="name">Severity</field>
      <field name="res_model">mgmt.severity</field>
      <field name="view_mode">tree,form</field>
    </record>

</odoo>
